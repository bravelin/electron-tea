{
    "name": "wisdom-tea-garden",
    "version": "1.0.0",
    "description": "a electron application for tea.yufengtek.com",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --win --ia32"
    },
    "build": {
        "directories": {
            "buildResources": "./build",
            "output": "./dist",
            "app": "./"
        },
        "compression": "maximum",
        "files": ["**/*", "build/icon.*"],
        "productName": "武夷山市智慧茗园数据平台",
        "copyright": "yufeng",
        "appId": "com.yufengtek",
        "win": {
            "target": "msi",
            "icon": "build/icon.ico"
        }
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron"
    ],
    "author": "yufengtek",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^5.0.0"
    }
